{
    "Tsh Commands": {
      "Basic Operations": {
        "Login": "tsh login <cluster-name>",
        "List nodes": "tsh nodes",
        "Get session": "tsh whoami",
        "Logout": "tsh logout"
      },
      "Cluster Management": {
        "List clusters": "tsh clusters",
        "Create cluster": "tsh create cluster <cluster-name>",
        "Delete cluster": "tsh delete cluster <cluster-name>"
      }
    }
  }