{
    "scp": {
      "file transfer": {
        "copy file to remote": "scp <local-file> <user>@<host>:<remote-path>",
        "copy file from remote": "scp <user>@<host>:<remote-file> <local-path>"
      },
      "directory transfer": {
        "copy directory to remote": "scp -r <local-dir> <user>@<host>:<remote-path>",
        "copy directory from remote": "scp -r <user>@<host>:<remote-dir> <local-path>"
      }
    }
  }