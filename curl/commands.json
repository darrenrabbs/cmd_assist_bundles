{
    "curl": {
      "basic": {
        "get request": "curl -X GET <url>",
        "post request": "curl -X POST <url> -d '<data>'",
        "put request": "curl -X PUT <url> -d '<data>'",
        "delete request": "curl -X DELETE <url>"
      },
      "headers": {
        "add header": "curl -H '<header>: <value>' <url>",
        "show headers": "curl -I <url>"
      },
      "authentication": {
        "basic auth": "curl -u <user>:<password> <url>",
        "bearer token": "curl -H 'Authorization: Bearer <token>' <url>"
      },
      "output": {
        "save to file": "curl <url> -o <file>",
        "show progress": "curl <url> -#"
      }
    }
  }