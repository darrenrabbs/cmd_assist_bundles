{
    "sed": {
      "text subs": {
        "substitute text": "sed 's/old/new/g' <file>",
        "delete lines": "sed '/pattern/d' <file>"
      },
      "in-place editing": {
        "edit file in-place": "sed -i 's/old/new/g' <file>",
        "backup before editing": "sed -i.bak 's/old/new/g' <file>"
      }
    }
  }