{
    "pulumi": {
      "setup": {
        "new project": "pulumi new <template>",
        "install dependencies": "pulumi install"
      },
      "deployment": {
        "deploy stack": "pulumi up",S
        "preview changes": "pulumi preview",
        "destroy Stack": "pulumi destroy"
      },
      "stack management": {
        "list stacks": "pulumi stack ls",
        "select stack": "pulumi stack select <stack-name>"
      },
      "configuration": {
        "set config": "pulumi config set <key> <value>",
        "get config": "pulumi config get <key>"
      }
    }
  }