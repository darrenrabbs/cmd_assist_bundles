{
    "ansible": {
      "setup": {
        "Check Version": "ansible --version",
        "List Inventory": "ansible-inventory --list"
      },
      "playbooks": {
        "Run Playbook": "ansible-playbook <playbook.yml>",
        "Check Playbook Syntax": "ansible-playbook --syntax-check <playbook.yml>"
      },
      "ad-hoc": {
        "Run Command on Host": "ansible <host> -m <module> -a <args>",
        "Ping Hosts": "ansible <host> -m ping"
      },
      "roles": {
        "Install Role": "ansible-galaxy install <role>",
        "Create Role": "ansible-galaxy init <role-name>"
      }
    }
  }